/* pages/car-detail/car-detail.wxss */
.container{
  width: 100%;
  overflow: hidden;
}
.swiper{
  width: 100%;
  height: 230rpx;
}
.slide-image{
  width: 100%;
  height: 230rpx;
}


.car-overview{
  padding: 0 30rpx;
}
.overview_line{
  height: 120rpx;
  justify-content: space-around;
  align-items: flex-start;
}


.setting_area{
  padding: 0 30rpx;
}
.setting_line{
  line-height: 85rpx;
  justify-content: space-between;
}
.setting_label,.setting_detail{
  font-size: 26rpx;
  font-weight: 500;
  color: #252621;
}
.tips_area{
  margin: 30rpx auto;
  justify-content: space-between;
}
.tips_block{
  width: 330rpx;
  height: 120rpx;
  padding: 12rpx 18rpx;
  background: #252621;
  border-radius: 12rpx;
}
.tips_text1{
  padding-left: 34rpx;
  font-size: 22rpx;
  color: #888888;
  background: url('data:image/png;base64,') left center no-repeat;
  background-size: 24rpx 24rpx;
}
.tips_text2{
  padding-left: 34rpx;
  font-size: 22rpx;
  color: #888888;
  background: url('data:image/png;base64,') left center no-repeat;
  background-size: 24rpx 24rpx;
}
.gold{
  font-size: 20rpx;
  color: #BA9478;
}

.service-line{
  line-height: 84rpx;
  justify-content: flex-start;
}
.service-title{
  padding: 0 40rpx;
  color: rgba(163, 163, 163, 1);
}


.gold-btn{
  width: 690rpx;
  line-height: 88rpx;
  margin: 100rpx auto 52rpx auto;
  background: linear-gradient(91deg, #BA9478, #FFE2BF);
  border-radius: 16rpx;
  color: #252621;
  font-size: 30rpx;
  text-align: center;
}


.split-line{
  width: 100%;
  height: 30rpx;
  background-color: #F6F6F6;
}


.gray_text{
  font-size: 24rpx;
  color: #888888;
}