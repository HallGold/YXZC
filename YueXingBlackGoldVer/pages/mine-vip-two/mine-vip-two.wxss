/* pages/mine-vip-two/mine-vip-two.wxss */
.top-card{
  width: 600rpx;
  height: 160rpx;
  padding: 30rpx;
  margin: 48rpx auto 0 auto;
  background: url('data:image/png;base64,');
  background-size: 100% 100%;
  border-radius: 16rpx 16rpx 0px 0px;
}
.register-card{
  width: 690rpx;
  margin: 0 auto;
  padding: 40rpx 30rpx;
  background: #FFFFFF;
  box-shadow: 1rpx 4rpx 10rpx 0px #ECECEC;
  border-radius: 16rpx;
}
.register-text1{
  width: 100%;
  color: #866464;
  font-size: 30rpx;
  font-weight: bold;
  text-align: center;
}
.register-text2{
  width: 100%;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  font-size: 30rpx;
  font-weight: bold;
}
.register-line{
  width: 100%;
  height: 95rpx;
  align-items: center;
  border-bottom: 2rpx solid #F6F6F6;
}
.register-label{
  width: 150rpx;
  flex-shrink: 0;
}
.register-input{
  flex-grow: 1;
}
.resiter-btn{
  width: 200rpx;
  color: #37ADEF;
}
.gold-btn{
  width: 690rpx;
  line-height: 88rpx;
  margin: 100rpx auto;
  background: linear-gradient(91deg, #BA9478, #FFE2BF);
  border-radius: 44rpx;
  color: #252621;
  font-size: 30rpx;
  text-align: center;
}

.success-mask{
  position: fixed;
  width: 100%;
  height: 100vh;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(59, 59, 59, .91);
  z-index: 1;
}
.success-pop{
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  z-index: 2;
}
.success-box{
  width: 580rpx;
  height: 200rpx;
  margin: 0 auto;
  justify-content: center;
  background: #FFFFFF;
  border-radius: 16rpx;
  text-align: center;
}
.success-text1{
  color: #3B3B3B;
  font-size: 30rpx;
  font-weight: bold;
}
.success-text2{
  margin-top: 20rpx;
  color: #DFDFDF;
  font-size: 24rpx;
}
.success-close{
  width: 40rpx;
  line-height: 40rpx;
  margin: 0 auto;
  border: 1rpx solid white;
  border-radius: 50%;
  text-align: center;
  color: white;
  font-size: 34rpx;
}